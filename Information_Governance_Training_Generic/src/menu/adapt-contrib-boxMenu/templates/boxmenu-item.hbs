{{!--<div class="menu-item-inner" aria-label="{{_globals._menu._boxmenu.menuItem}}" {{#if _globals._menu._boxmenu.menuItem}}tabindex="0"{{/if}}>--}}
<div class="menu-item-inner" aria-label="{{_globals._menu._boxmenu.menuItem}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" {{#if _globals._menu._boxmenu.menuItem}}tabindex="0"{{/if}} role="button" class="{{#if _isVisited}}visited{{/if}} {{#if _isLocked}}disabled{{/if}} accessible-text-block" {{#if _isLocked}}disabled="disabled"{{/if}}>
    <div class="menu-item-graphic">
        {{#if _graphic.src}}
            <img  id="graphic-{{_id}}" src="{{_graphic.src}}" alt="{{_graphic.alt}}" />
        {{/if}}
    </div>
    <div class="menu-item-title">
        <div id="menu-item-title-base" class="menu-item-title-base">
            <div id="menu-item-title-left-base" class="menu-item-title-left-base menu-item-title-left-base-normal"></div>
            <div id="menu-item-title-right-base" class="menu-item-title-right-base menu-item-title-right-base-normal"></div>
        </div>
        <div id="menu-item-title-inner-{{_id}}" class="menu-item-title-inner h3 accessible-text-block" role="heading" aria-level="2" tabindex="0">{{{displayTitle}}}</div>
    </div>
    {{!--<div class="menu-item-body">
        <div class="menu-item-body-inner">{{{a11y_text body}}}</div>
    </div>--}}
    {{!--<div class="menu-item-button">
        <button aria-label="{{linkText}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" class="{{#if _isVisited}}visited{{/if}} {{#if _isLocked}}disabled{{/if}} accessible-text-block" {{#if _isLocked}}disabled="disabled"{{/if}}>
            {{{linkText}}}
        </button>
        {{#if duration}}
            <span class="menu-item-duration accessible-text-block" role="region" tabindex="0">{{#if _globals._menu._boxmenu.durationLabel}}{{{_globals._menu._boxmenu.durationLabel}}}{{/if}} {{{duration}}}</span>
        {{/if}}
    </div>--}}
</div>
